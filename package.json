{
  "private": true,
  "scripts": {
    "build": "npm run build:ssg && npm run rss",
    "build:spa": "valaxy build",
    "build:ssg": "valaxy build --ssg",
    "dev": "valaxy",
    "rss": "valaxy rss",
    "serve": "vite preview",
    "prepare": "simple-git-hooks",
    "up": "taze latest -I",
    "lint": "eslint .",
    "prettier": "prettier --write ."
  },
  "dependencies": {
    "valaxy": "0.14.53",
    "valaxy-theme-yun": "0.14.53"
  },
  "devDependencies": {
    "@ayingott/eslint-config": "^0.2.7",
    "eslint": "^8.47.0",
    "lint-staged": "^14.0.0",
    "pnpm": "^8.6.12",
    "prettier": "^3.0.2",
    "simple-git-hooks": "^2.9.0",
    "taze": "^0.11.2",
    "typescript": "^5.1.6"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "eslintConfig": {
    "extends": "@ayingott/eslint-config"
  }
}
