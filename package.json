{
  "name": "nuxt-app",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "lint": "eslint .",
    "postinstall": "nuxt prepare",
    "prepare": "simple-git-hooks",
    "prettier": "prettier --write .",
    "preview": "nuxt preview",
    "up": "taze latest -I -f"
  },
  "dependencies": {
    "@unocss/reset": "^0.59.0",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@ayingott/eslint-config": "^0.9.0",
    "@ayingott/prettier-config": "^0.3.0",
    "@iconify-json/line-md": "^1.1.37",
    "@nuxtjs/color-mode": "^3.3.3",
    "@unocss/nuxt": "^0.59.0",
    "@vueuse/core": "^10.9.0",
    "@vueuse/nuxt": "^10.9.0",
    "eslint": "^9.0.0",
    "lint-staged": "^15.2.2",
    "nuxt": "^3.11.2",
    "prettier": "^3.2.5",
    "simple-git-hooks": "^2.11.1",
    "taze": "^0.13.3",
    "unocss": "^0.59.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged"
  },
  "lint-staged": {
    "*": [
      "eslint --fix",
      "prettier --write --ignore-unknown"
    ]
  },
  "prettier": "@ayingott/prettier-config"
}
