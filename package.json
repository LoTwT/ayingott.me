  "dependencies": {
    "@nuxt/content": "^2.12.0",
    "@unocss/reset": "^0.58.5",
    "daisyui": "3.9.4",
    "unocss-preset-daisy": "^7.0.0"
  },
  "devDependencies": {
    "@ayingott/eslint-config": "^0.8.0",
    "@ayingott/prettier-config": "^0.2.0",
    "@iconify-json/carbon": "^1.1.30",
    "@iconify-json/icon-park-outline": "^1.1.15",
    "@nuxt/devtools": "^1.0.8",
    "@nuxtjs/i18n": "8.0.0-rc.5",
    "@types/node": "^20.11.19",
    "@unocss/nuxt": "^0.58.5",
    "@vueuse/core": "^10.8.0",
    "@vueuse/nuxt": "^10.8.0",
    "eslint": "^8.56.0",
    "lint-staged": "^15.2.2",
    "nuxt": "^3.10.2",
    "prettier": "^3.2.5",
