  },
  "devDependencies": {
    "@ayingott/eslint-config": "^0.7.4",
    "@ayingott/prettier-config": "^0.1.2",
    "@iconify-json/carbon": "^1.1.23",
    "@nuxt/devtools": "^1.0.4",
    "@nuxtjs/i18n": "8.0.0-rc.5",
    "@types/node": "^20.10.0",
    "@unocss/nuxt": "^0.57.7",
    "@vueuse/core": "^10.6.1",
    "@vueuse/nuxt": "^10.6.1",
    "eslint": "^8.54.0",
    "lint-staged": "^15.1.0",
    "nuxt": "^3.8.2",
    "prettier": "^3.1.0",
    "simple-git-hooks": "^2.9.0",
    "taze": "^0.12.3",
    "vue": "^3.3.9",
    "vue-router": "^4.2.5"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged"
  },
